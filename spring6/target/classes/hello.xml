<?xml version="1.0" encoding="UTF-8"?>
<beans 

    xmlns="http://www.springframework.org/schema/beans" 

    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 

    xmlns:p="http://www.springframework.org/schema/p" 

    xsi:schemaLocation="http://www.springframework.org/schema/beans 

               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 

  

<bean id="hello" class="com.pack.HelloWorld">
<property name="msg1" value="dfgxf"></property> 
<property name="msg2" value="qwerty"></property> <!-- id acts as an object of the bean-->
</bean> 
<bean id="hello1" class="com.pack.HelloIndia" parent="hello"><!-- hello refers to the id of the parent bean -->
<property name="msg3" value="diksha"></property> 

</bean>


</beans> 